<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ui.ContentProviderActivity">

    <Button
        android:id="@+id/bt_get_call_log"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/get_call_log" />

    <TextView
        android:id="@+id/tv_number_show"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/bt_get_my_provider"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/get_my_provider" />

    <TextView
        android:id="@+id/tv_database_show"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</LinearLayout>